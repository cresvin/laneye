{
  "name": "laneye",
  "version": "1.0.1",
  "type": "module",
  "bin": "./dist/index.js",
  "description": "A CLI tool that analyzes a project directory to determine the percentage of different programming languages used, by examining file extensions and excluding common non-code files.",
  "scripts": {
    "dev": "tsc -w",
    "buiid": "tsc"
  },
  "repository": "https://github.com/cresvin/laneye.git",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "file system",
    "directory traversal",
    "recursive search",
    "file extension",
    "percentage calculation",
    "ignore extensions",
    "nodejs",
    "fs module",
    "javascript",
    "file filtering",
    "subdirectories",
    "list files",
    "path handling"
  ],
  "author": "Oliwier Tkacz <otkacz.buissnes@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "nanospinner": "^1.1.0"
  }
}
